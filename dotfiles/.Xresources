!!!!!! XConfig file !!!!!!


!!! ROFI !!!
! ! Use extended color schem
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #D8DEE9 , argb:00000000,  #FAC863 ,  #000000
! ! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #F99157 , argb:00000000,  #F99157 ,  #000000
! ! Color scheme for active row
rofi.color-active:                   argb:00000000,  #000000 , argb:00000000,  #000000 ,  #000000
! ! Color scheme window
rofi.color-window:                   argb:ee222222,  #FAC863 ,  #FAC863
! ! Separator style (none, dash, solid)
rofi.separator-style:                dash



!!! URxvt-unicode !!!
! Terminal
URxvt*termName: screen-256color
! Transparency
urxvt*transparent : true
urxvt*shading : 20
! Font
urxvt*font: xft:Inconsolata:size=13,xft: Inconsolata\ for\ Powerline:size=13		
urxvt*boldFont: xft:Inconsolata:size=13
urxvt*italicFont: xft:Inconsolata:size=13
urxvt*bolditalicFont: xft:Inconsolata:size=13
urxvt*scrollBar: false
! Settings
urxvt*internalBorder: 3
URxvt*cursorUnderline: true
URxvt*cursorBlink: true
URxvt*savelines: 10000
! Optionnal settings
URxvt.letterSpace:1
! Perl extensions
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.colorBD: white
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher
URxvt*perl-ext:
URxvt*perl-ext-common:
